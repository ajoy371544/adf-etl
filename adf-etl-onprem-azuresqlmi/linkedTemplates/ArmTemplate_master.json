{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-etl-onprem-azuresqlmi"
		},
		"AzureSqlMINotional_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlMINotional'"
		},
		"OnPremDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'OnPremDatabase'"
		},
		"OnPremDatabase_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'OnPremDatabase'"
		},
		"OnPremDatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "LAPTOP-8OSIK8HA\\Ajoy Hazra"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-etl-onprem-azuresqlmi_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlMINotional_connectionString": {
						"value": "[parameters('AzureSqlMINotional_connectionString')]"
					},
					"OnPremDatabase_connectionString": {
						"value": "[parameters('OnPremDatabase_connectionString')]"
					},
					"OnPremDatabase_password": {
						"value": "[parameters('OnPremDatabase_password')]"
					},
					"OnPremDatabase_properties_typeProperties_userName": {
						"value": "[parameters('OnPremDatabase_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}